		"lanmode": false,
		"query": true,
		"rcon": false,
		"logqueries": false,
		"sleep": 5,
		"maxnpc": 0,
		"stream_rate": 1000,
		"stream_distance": 200,
		"onfoot_rate": 30,
		"incar_rate": 30,
		"weapon_rate": 30,
		"chatlogging": true,
		"timestamp": true,
		"messageholelimit": 3000,
		"messageslimit": 500,
		"ackslimit": 3000,
		"playertimeout": 10000,
		"minconnectiontime": 0,
		"lagcompmode": 1,
		"connseedtime": 300000,
		"db_logging": false,
		"db_log_queries": false,
		"conncookies": true,
		"cookielogging": false,
		"output": true
	}
